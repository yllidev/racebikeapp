<h1>Contest</h1>


<%= form_for :contest, :url => '/contest' , :html => {:class => "nifty_form"} do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :slogan %>
  <%= f.text_area :slogan %>
  <%= submit_tag "Submit" %>
<% end %>